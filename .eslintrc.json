{
	"extends": ["next", "prettier"],
	"rules": {
		"no-undef": "warn",
		"no-unused-vars": "off",
		"@next/next/no-img-element": "off",
		"react/no-unescaped-entities": "off"
	},
	"env": {
		"es6": true
	},
	"overrides": [
		{
			"files": ["*.ts", "*.tsx"],
			"rules": {
				"no-undef": "off"
			}
		}
	]
}
